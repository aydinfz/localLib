<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Title</title>
  </head>
  <body>
    <h1><%= title %></h1>

    <ul>
      <% if (author_list.length > 0) { %> <%
      author_list.forEach(function(author) { %>
      <li>
        <a href="<%= author.url %>"><%= author.name %></a> (<%=
        author.due_birth_formatted %> - <%= author.due_death_formatted %>)
      </li>
      <% }); %> <% } else { %>
      <li>There are no authors.</li>
      <% } %>
    </ul>
  </body>
</html>
